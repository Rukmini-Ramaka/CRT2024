#include <stdio.h>

int main() {
    int P;
    scanf("%d", &P);

    if (P < 100 || P > 200) {
        printf("INVALID INPUT\n");
    } else {
        int perGroup = P / 4;
        int remaining = P % 4;

        printf("%d\n", perGroup);
        printf("%d\n", perGroup);
        printf("%d\n", perGroup);
        printf("%d\n", perGroup + remaining);
    }

    return 0;
}
